<h1> Users List from API</h1>

<ul class="list-group">
  {{#each users}}

    <li class="list-group-item">
      <input class="form-check-input me-1" type="checkbox" />
      <label class="form-check-label">{{firstName}}
        -
        {{lastName}}
        -
        {{email}}</label>
    </li>

  {{/each}}
</ul>